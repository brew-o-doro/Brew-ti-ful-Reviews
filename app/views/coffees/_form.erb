<%= form_with model: @coffee do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <br>
  <br>
  <%= form.label :brand %>
  <%= form.text_field :brand %>
  <br>
  <br>
  <%= form.label :description %>
  <%= form.text_field :description %>
  <br>
  <br>
  <%= form.label :roast_level %>
  <%= form.text_field :roast_level %>
  <br>
  <br>
  <%= form.label :caffeine_content %>
  <%= form.text_field :caffeine_content %>
  <br>
  <%= form.submit "Save Coffee" %>
<% end %>
