<%= form_with model: @review do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <br>
  <br>
  <%= form.label :rating %>
  <%= form.number_field :rating, min: 0, max: 10 %>
  <br>
  <br>
  <%= form.label :description %>
  <%= form.text_field :description %>
  <br>
  <br>
  <%= form.submit "Save Review" %>
<% end %>
